@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutFront.cshtml";
}

<div class="row">
    <div class="col-md-4 col-md-offset-4">
        <div class="login-panel panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title" id="lblTitile">Please Sign In</h3>
            </div>
            <div class="panel-body" id="template_content">

            </div>
        </div>
    </div>
</div>

<input type="hidden" id="hndBaseUrl" value="@ECart.Utilities.Paths.ApiBase" />

@{Html.RenderAction("_Login"); }
@{Html.RenderAction("_Register"); }
@{Html.RenderAction("_ForgetPassword"); }
@{Html.RenderAction("_EmailValidation"); }



@section moduleScript{
     <script src="~/asset/ts/appts/comman/Enums.js"></script>
    <script src="~/asset/ts/appts/comman/ajax.js"></script>
    <script src="~/asset/ts/appts/user/userAuthontication.js"></script>
}